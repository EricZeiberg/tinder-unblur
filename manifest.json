{
    "author": "ylwsnw",
    "content_scripts": [
        {
            "matches": [
                "*://tinder.com/app/*"
            ],
            "js": [
                "scripts/unblur.js"
            ]
        }
    ],
    "description": "unblurs the thumbnail of the last 10 people who liked you on tinder.com",
    "icons": {
        "16": "icons/icon_16.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "manifest_version": 2,
    "name": "Tinder Unblur",
    "permissions": [
        "*://tinder.com/app/*"
    ],
    "version": "1.0"
  }